{
  "name": "iracing-planner",
  "productName": "AMG's iRacing Planner",
  "version": "2.2.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --code-coverage",
    "electron": "ng build && electron .",
    "electron-package-win": "ng build && electron-packager . iracing-planner --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.ProductName=\"AMG's iRacing Planner\"",
    "electron-package-osx": "ng build && electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "electron-package-linux": "ng build && electron-packager . iracing-planner --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "lint": "ng lint",
    "zip": "zip-build release-builds/iracing-planner-win32-x64 release-builds -t amgiRacingPlanner_v%VERSION%.%EXT%"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.3",
    "@angular/common": "^15.0.3",
    "@angular/compiler": "^15.0.3",
    "@angular/core": "^15.0.3",
    "@angular/forms": "^15.0.3",
    "@angular/material": "^15.0.2",
    "@angular/platform-browser": "^15.0.3",
    "@angular/platform-browser-dynamic": "^15.0.3",
    "@angular/router": "^15.0.3",
    "compare-versions": "^6.0.0-rc.1",
    "crypto-js": "^4.1.1",
    "electron-store": "^8.1.0",
    "jasmine": "^4.6.0",
    "node-fetch": "^3.3.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.0.3",
    "@angular/compiler-cli": "^15.0.3",
    "@types/jasmine": "~4.3.1",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "electron": "^23.2.1",
    "electron-packager": "^17.1.1",
    "eslint": "^8.37.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.3",
    "zip-build": "^1.8.0"
  }
}
