{
  "name": "iracing-planner",
  "productName": "AMG's iRacing Planner",
  "version": "2.0.1",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build && electron .",
    "electron-package-win": "ng build && electron-packager . iracing-planner --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.ProductName=\"AMG's iRacing Planner\"",
    "electron-package-osx": "ng build && electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "electron-package-linux": "ng build && electron-packager . iracing-planner --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "lint": "ng lint",
    "zip": "zip-build release-builds/iracing-planner-win32-x64 release-builds -t amgiRacingPlanner_v%VERSION%.%EXT%"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.5",
    "@angular/common": "^14.2.5",
    "@angular/compiler": "^14.2.5",
    "@angular/core": "^14.2.5",
    "@angular/forms": "^14.2.5",
    "@angular/material": "^14.2.4",
    "@angular/platform-browser": "^14.2.5",
    "@angular/platform-browser-dynamic": "^14.2.5",
    "@angular/router": "^14.2.5",
    "electron-store": "^8.1.0",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.5",
    "@angular/compiler-cli": "^14.2.5",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "electron": "^21.1.0",
    "electron-packager": "^16.0.0",
    "eslint": "^8.25.0",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.4",
    "zip-build": "^1.7.0"
  }
}
